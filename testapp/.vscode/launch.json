{
    "version": "0.1.0",
    "debugServer": "4712",
    "configurations": [
        {
            "name": "test chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/index.html",
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot"
        },
        {
            "name": "launch for file",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceRoot}/wwwroot/index.html",
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot"
        },
        {
            "name": "attach to chrome",
            "type": "chrome",
            "port": 9222,
            "request": "attach",
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "url": "http://localhost:8080"
        }
    ]
}
